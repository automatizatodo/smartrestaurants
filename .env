
# For local development, point this to your downloaded Google Service Account JSON key file
# Place the JSON key file (e.g., gastronomic-canvas-credentials.json) in your project root
# or any accessible path, and update this variable accordingly.
GOOGLE_APPLICATION_CREDENTIALS=gastronomic-canvas-credentials.json.json

# Your Google Calendar ID (e.g., 'primary' or your_calendar_email@group.calendar.google.com)
GOOGLE_CALENDAR_ID=primary

# The URL for your local development server (adjust port if different)
# Your package.json uses port 9003 for `npm run dev`
NEXT_PUBLIC_APP_URL=http://localhost:9003

# Your Google Analytics Measurement ID (G-XXXXXXXXXX)
NEXT_PUBLIC_GA_MEASUREMENT_ID="G-ZEF2C45GJ7"

# Google Sheet CSV URLs - IMPORTANT: Replace these with your actual published CSV URLs
# URL for the main menu sheet (File > Share > Publish to web > Select sheet > CSV)
GOOGLE_SHEET_MENU_CSV_URL="YOUR_MAIN_MENU_SHEET_PUBLISHED_CSV_URL_HERE"
# URL for the "preciosmenu" sheet (File > Share > Publish to web > Select "preciosmenu" sheet > CSV)
GOOGLE_SHEET_PRICES_CSV_URL="YOUR_PRICES_MENU_SHEET_PUBLISHED_CSV_URL_HERE"
